{
  "name": "appsbyv/v-chatgpt-social-status-feeds",
  "description": "Manage, schedule, and distribute social media status feeds.",
  "license": "MIT",
  "type": "project",
  "require": {
  "php": "^8.1",
    "phpmailer/phpmailer": "^6.8",
    "doctrine/dbal": "^3.6",
    "nikic/fast-route": "^1.3",
    "openai-php/client": "*",
    "respect/validation": "*",
    "symfony/http-client": "^7.3",
    "nyholm/psr7": "^1.8",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
  "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^4.0"
  }
}
